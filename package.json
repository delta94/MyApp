{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "ios:prod": "gulp set --env=prod && react-native run-ios",
    "ios:staging": "gulp set --env=staging && react-native run-ios",
    "android:prod": "gulp set --env=prod && react-native run-android",
    "android:staging": "gulp set --env=staging && react-native run-android",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "@eva-design/eva": "^2.0.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.7.3",
    "@react-navigation/drawer": "^5.8.7",
    "@react-navigation/material-top-tabs": "^5.2.16",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@ui-kitten/components": "^5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "formik": "^2.1.5",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "react": "16.13.1",
    "react-hook-form": "^6.4.0",
    "react-hook-form-input": "^1.1.10",
    "react-native": "0.63.2",
    "react-native-device-info": "^5.6.3",
    "react-native-dotenv": "0.2.0",
    "react-native-firebase": "^5.6.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-reanimated": "^1.10.1",
    "react-native-redash": "^14.2.4",
    "react-native-responsive-screen": "^1.4.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-native": "^0.63.8",
    "@types/react-test-renderer": "^16.9.3",
    "@ui-kitten/metro-config": "5.0.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "gulp": "^4.0.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.2",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.11.0",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
